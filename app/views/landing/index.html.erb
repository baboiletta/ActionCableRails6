<h1>Landing#index</h1>
<p>Find me in app/views/landing/index.html.erb</p>
<a href="/users/sign_out" data-method="delete">
  Sign out
</a>

<div id="msg">

</div>

<%= form_for @message, remote: true do |f| %>
  <%= f.text_field :body %>
  <%= f.number_field :resource_id %>
  <%= f.hidden_field :enrollment_id, value: current_user.enrollments.first.id %>
  <%= f.submit %>
<% end %>